#: src/help.c:104
msgid "Usage: anubis [options]\n"
msgstr "Використання: anubis [параметри]\n"

#: src/help.c:105
msgid ""
" -b, --bind [HOST:]PORT       Specify the TCP port on which GNU Anubis "
"listens\n"
"                              for connections. The default HOST is "
"INADDR_ANY,\n"
"                              and default PORT is 24 (private mail system)."
msgstr ""
" -b, --bind [МАШИНА:]ПОРТ     Вкажіть TCP порт на якому GNU Anubis "
"очікуватиме\n"
"                              з'єднання. Типово МАШИНА це INADDR_ANY,\n"
"                              та ПОРТ це 24 (приватна поштова система)."

#: src/help.c:108
msgid ""
" -r, --remote-mta HOST[:PORT] Specify a remote SMTP host name or IP "
"address.\n"
"                              The default PORT number is 25."
msgstr ""
" -r, --remote-mta МАШИНА[:ПОРТ] Вкажіть ім'я віддаленої SMTP машини або її\n"
"                                адресу IP. Типовий ПОРТ -- 25."

#: src/help.c:110
msgid ""
" -l, --local-mta FILE         Execute a local SMTP server, which works on\n"
"                              standard input and output (inetd-type "
"program).\n"
"                              This option excludes the '--remote-mta' option."
msgstr ""
" -l, --local-mta ФАЙЛ         Виконати локальну програму SMTP, яка працює з\n"
"                              стандартним вводом та виводом (програма типу\n"
"                              inetd).\n"
"                              Ця опція виключає '--remote-mta'."

#: src/help.c:113
msgid " -m, --mode=MODE              Select operation mode."
msgstr " -m, --mode=РЕЖИМ             Вибір режиму роботи."

#: src/help.c:114
#, fuzzy
msgid ""
"                              MODE is one of \"transparent\", \"auth\" or "
"\"mda\"."
msgstr "                              РЕЖИМ є \"transparent\" або \"auth\"."

#: src/help.c:115
msgid " -f, --foreground             Foreground mode."
msgstr " -f, --foreground             Робота на тлі."

#: src/help.c:116
msgid " --from=EMAIL                 Specify sender address (implies MDA mode)"
msgstr ""
" --from=АДРЕС                 Вказує адресу відправника (вмикає режим MDA)"

#: src/help.c:117
msgid ""
" -i, --stdio                  Use the SMTP protocol (OMP/Tunnel) as "
"described\n"
"                              in RFC 821 on standard input and output."
msgstr ""
" -i, --stdio                  Використовувати SMTP протокол (OMP/Tunnel),\n"
"                              описаний в RFC 821, на стандартному вводі\n"
"                              і виводі."

