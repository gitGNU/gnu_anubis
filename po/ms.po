#: src/help.c:104
msgid "Usage: anubis [options]\n"
msgstr "Penggunaan: anubis [pilihan]\n"

#: src/help.c:105
msgid ""
" -b, --bind [HOST:]PORT       Specify the TCP port on which GNU Anubis listens\n"
"                              for connections. The default HOST is INADDR_ANY,\n"
"                              and default PORT is 24 (private mail system)."
msgstr ""
" -b, --bind [HOS:]PORT        Nyatakan port TCP dimana GNU Anubis mendengar\n"
"                              untuk sambungan. HOS default adalah INADDR_ANY,\n"
"                              dan PORT default ialah 24 (sistem mel persendirian)."

#: src/help.c:108
msgid ""
" -r, --remote-mta HOST[:PORT] Specify a remote SMTP host name or IP address.\n"
"                              The default PORT number is 25."
msgstr ""
" -r, --remote-mta HOS[:PORT]  Nyatakan nama hos jauh SMTP atau alamatIP.\n"
"                              Nombor PORT default adalah 25."

#: src/help.c:110
msgid ""
" -l, --local-mta FILE         Execute a local SMTP server, which works on\n"
"                              standard input and output (inetd-type program).\n"
"                              This option excludes the '--remote-mta' option."
msgstr ""
" -l, --local-mta FAIL         Laksanakan pelayan SMTP tempatan, yang berkerja pada\n"
"                              masukan dan keluaran piawai (program jenis-inetd).\n"
"                              Pilihan ini tidak termasuk pilihan '--remote-mta'."

#: src/help.c:113
msgid " -m, --mode=MODE              Select operation mode."
msgstr " -m, --mode=MOD               Pilih mod operasi."

#: src/help.c:114
msgid "                              MODE is either \"transparent\" or \"auth\""
msgstr "                              MOD adalah sama ada \"transparent\" atau \"auth\""

#: src/help.c:115
msgid " -f, --foreground             Foreground mode."
msgstr " -f, --foreground             Mod latardepan."

#: src/help.c:116
msgid ""
" -i, --stdio                  Use the SMTP protocol (OMP/Tunnel) as described\n"
"                              in RFC 821 on standard input and output."
msgstr ""
" -i, --stdio                  Guna protokol SMTP (OMP/Tunnel) seperti diterangkan\n"
"                              didalam RFC 821 pada masukan dan keluaran piawai."

